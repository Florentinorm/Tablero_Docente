                                                    </svg>
                                                </a>
                                            </div>
                                            {{!-- Termina Actividad --}}
                                        </div>
                                        {{/if_eq_string}}
                                        {{/each}}
                                        {{!-- Examen --}}
                                        {{#each @root.examen}}
                                        {{#if_eq_string this.cvTema ../this._id}}
